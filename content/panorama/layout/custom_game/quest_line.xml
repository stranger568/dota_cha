<root>
	<styles>
		<include src="s2r://panorama/styles/custom_game/quest_line.vcss_c" />
		<include src="s2r://panorama/styles/custom_game/ready_button.vcss_c" />
	</styles>
	<scripts>
		<include src="s2r://panorama/scripts/custom_game/quest_line.vjs_c" />
		<include src="s2r://panorama/scripts/custom_game/ready_button.vjs_c" />
		<include src="s2r://panorama/scripts/custom_game/shared_scoreboard_updater.vjs_c" />
		<include src="s2r://panorama/scripts/custom_game/util.vjs_c" />
	</scripts>
	<Panel style="width:100%;height:100%;" hittest="false">
		<Panel hittest="false" id="QuestMain">
			<Panel hittest="false" id="QuestPanel">
				<Panel class="WaveInfoDuration">
					<Label class="WaveInfoDurationCurrentState" id="WaveInfoDurationCurrentState" text="" />
					<Label class="RoundTime" id="RoundTime" text="0:00" />
				</Panel> 
				<Panel class="RoundInfo">
					<Panel class="RoundInfoNameAndAbilities">
						<Panel class="RoundPanel" id="RoundPanel" />
						<Label class="RoundLabel" id="RoundLabel" text="-" />
						<Label class="RoundName" id="RoundName" text="" />
						<Panel class="AbilitiesRound" id="AbilitiesRound" />
					</Panel>
					<Panel class="LinePanelAndReadyButton">
						<Panel class="LinePanel">
							<Panel class="LinePanelBG" />
							<Panel class="LinePanelActive" id="LinePanelActive" />
						</Panel>
						<Panel class="ReadyButtonNew" id="ReadyButtonNew" onactivate="PlayerReady();">
							<Label class="ReadyTextNew" text="#Ready" />
						</Panel>
					</Panel>
					<Label class="KillsInfo" id="KillsInfo" text="Убито 0/6" />
				</Panel>
			</Panel>
		</Panel>
	</Panel>
</root>
