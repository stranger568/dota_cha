<root>
	<styles>
		<include src="s2r://panorama/styles/dotastyles.css" />
		<include src="file://{resources}/styles/custom_game/donate.css" />
	</styles>
	<scripts>
		<include src="file://{resources}/scripts/custom_game/donate.js" />
		<include src="s2r://panorama/scripts/custom_game/util.vjs_c" />
	</scripts>

	<Panel style="width:100%;height:100%;" hittest="false">
		<Panel id="SmilesButton" onactivate="ToggleSmiles();" />
		<Panel class="SmilesWindow sethidden" id="SmilesWindow" hittest="false" />

		<Panel class="BattlePassWindow sethidden" id="BattlePassWindow" hittest="false">
			<Panel class="HeaderDonate">	
				<Panel class="DonateButtons" id="DonateButtons">
					<Panel class="DonateButton" onactivate="SetMenu('DonateButtonShop', 'DonateWindowBattleShop')" id="DonateButtonShop">
						<Label class="DonateButtonLabel" text="#button_shop" />
					</Panel>
					<Panel class="DonateButton" onactivate="SetMenu('DonateButtonBattlePass', 'DonateWindowBattlePass')" id="DonateButtonBattlePass">
						<Label class="DonateButtonLabel" text="#button_bp" />
					</Panel>
					<Panel class="DonateButton" onactivate="SetMenu('DonateButtonProfile', 'DonateWindowBattleProfile')" id="DonateButtonProfile">
						<Label class="DonateButtonLabel" text="#button_profile" />
					</Panel>
					<Panel class="DonateButton" onactivate="SetMenu('DonateButtonRewards', 'DonateWindowBattleRewards')" id="DonateButtonRewards">
						<Label class="DonateButtonLabel" text="#button_rewards" />
					</Panel>
				</Panel>
				<Panel class="ArenaCoinPanel">
					<Panel class="ArenaCoinIcon" />
					<Panel class="PanelBack">
						<Label class="ArenaCoinLabel" id="ArenaCoinLabel" text="0" />
						<Panel class="CurrencyDonateButton"  onactivate="OpenBattlePassInfo('BuyBattlePassWindow_coinshop');" />
					</Panel>
				</Panel>
				<Panel class="DonateCoinPanel">
					<Panel class="DonateCoinIcon" />
					<Panel class="PanelBack">
						<Label class="DonateCoinLabel" id="DonateCoinLabel" text="0" />
						<Panel class="CurrencyDonateButton"  onactivate="OpenBattlePassInfo('BuyBattlePassWindow_coinshop');" />
					</Panel>
				</Panel>
				<Panel class="Close" onactivate="CloseWindowsBPFast();"/>
			</Panel>
			<Panel class="DonateWindowBasic" id="DonateWindowBattlePass">
				<Panel class="BattlePassInformation">
					<Panel class="BattlePassInformationHeader">
						<Label class="BattlePassInformationHeaderLabel" text="#how_to_you" />
					</Panel>
					<Panel class="BattlePassInformationBody">
						<Panel class="BattlePassInformationBodyImage" id="BattlePassInformationBodyImage" />
						<Panel class="BattlePassInformationBodyInfo">
							<Label class="BattlePassInformationBodyInfoName" text="Battle Pass" />
							<Label class="BattlePassInformationBodyInfoDescr" id="BattlePassInformationBodyInfoDescr" text="" />
							<Panel class="BattlePassInformationBodyInfoNavigation">
								<Panel class="NavigationArrowLeft" onactivate="BattlePassInfoSwap('left');" />
								<Panel class="NavigationWidgets" id="NavigationWidgets">
									<Panel class="NavigationWidget NavigationWidget_Active" />
									<Panel class="NavigationWidget" />
									<Panel class="NavigationWidget" />
									<Panel class="NavigationWidget" />
									<Panel class="NavigationWidget" />
									<Panel class="NavigationWidget" />
									<Panel class="NavigationWidget" />
									<Panel class="NavigationWidget" />
									<Panel class="NavigationWidget" />
								</Panel>
								<Panel class="NavigationArrowRight" onactivate="BattlePassInfoSwap('right');" />
							</Panel>
						</Panel>
					</Panel>
				</Panel>

				<Panel class="BattlePassInformationBuy">
					<Panel class="BattlePassInformationHeader">
						<Label class="BattlePassInformationHeaderLabel" text="#bp_inf_1" />
					</Panel>
					<Panel class="BattlePassInformationBody" style="flow-children:down;">
						<Panel class="BattlePassBlockInfo" style="margin-top:12px;">
							<Label class="BattlePassBlockInfoMain" text="#bp_inf_2" />
							<Label class="BattlePassBlockInfoMini" text="#bp_inf_3" />
						</Panel>
						<Panel class="BattlePassBlockInfo">
							<Label class="BattlePassBlockInfoMain" text="#bp_inf_4" />
							<Panel class="BattlePassBlockInfoTochka">
								<Panel class="TochkaInfo"/>
								<Label class="BattlePassBlockInfoMini" text="#bp_inf_5" />
							</Panel>
							<Panel class="BattlePassBlockInfoTochka">
								<Panel class="TochkaInfoMax"/>
								<Label class="BattlePassBlockInfoMini" text="#bp_inf_6" />
							</Panel>
						</Panel>
						<Panel class="BattlePassBlockInfo" id="DurationBattlePassPrice">
							<Label class="BattlePassBlockInfoMain" text="#bp_inf_7" />
							<Panel class="BattlePassPricePanel">
								<RadioButton onactivate="UpdateBattlePassPrice('0');" lass="RadioBPrice" group="BPPrice" selected="true" />
								<Label class="BpPriceLabel" html="true" text="Battle Pass на 1 месяц" />
							</Panel>
							<Panel class="BattlePassPricePanel">
								<RadioButton onactivate="UpdateBattlePassPrice('1');" class="RadioBPrice" group="BPPrice" />
								<Label class="BpPriceLabel" html="true" text="Battle Pass на 3 месяца" />
							</Panel>
							<Panel class="BattlePassPricePanel">
								<RadioButton onactivate="UpdateBattlePassPrice('2');" class="RadioBPrice" group="BPPrice" />
								<Label class="BpPriceLabel" html="true" text="Battle Pass на 6 месяцев" />
							</Panel>
							<Panel class="BattlePassPricePanel">
								<RadioButton onactivate="UpdateBattlePassPrice('3');" class="RadioBPrice" group="BPPrice" />
								<Label class="BpPriceLabel" html="true" text="Battle Pass на 12 месяцев" />
							</Panel>
						</Panel>
						<Panel class="BattlePassBlockInfo">
							<Panel class="BuyBattlePassButtons" id="BuyBattlePassButtons">
								<Panel class="BuyBattlePassButtonRed" id="BuyBattlePassButtonRed">
									<Panel style="flow-children:right;align:center center;">
										<Label class="BuyBattlePassButtonRedPrice" id="BuyBattlePassButtonRedPrice" text="0" />
										<Panel class="BuyBattlePassButtonRedPriceIcon" />
									</Panel>
								</Panel>
								<Panel class="BuyBattlePassButtonBlue" id="BuyBattlePassButtonBlue">
									<Panel style="flow-children:right;align:center center;">
										<Label class="BuyBattlePassButtonBluePrice" id="BuyBattlePassButtonBluePrice" text="0" />
										<Panel class="BuyBattlePassButtonBluePriceIcon" />
									</Panel>
								</Panel>
							</Panel>
							<Label class="BattlePassDaysDuration" id="BattlePassDaysDuration" text="" />
						</Panel>
					</Panel>
				</Panel>
			</Panel>

			<Panel class="DonateWindowBasic" id="DonateWindowBattleRewards">
				<Panel class="RewardsMain" id="RewardsMain" hittest="false">

				</Panel>
			</Panel>

			<Panel class="DonateWindowBasic" id="DonateWindowBattleProfile">
				<Panel class="ProfileColumnStart" >
					<Panel class="ProfileAndQuests">
						<Panel class="PlayerProfile">
							<Panel class="NickNameAvatarRating">
								<DOTAAvatarImage steamid="local" class="AvatarProfile" />
								<Panel class="PlayerNameAndRating">
									<DOTAUserName steamid="local" class="NameProfile"/>
									<Label class="PlayerRatingProfile" id="PlayerRatingProfile" text="Rating 0" />
								</Panel>
								<Panel class="PlayerRankProfile" id="PlayerRankProfile">
									<Label class="PlayerRankProfilePlace" id="PlayerRankProfilePlace" text="" />
								</Panel>
							</Panel>
							<Panel class="ArenaLevelPanel">
								<Label class="ArenaLevel" id="ArenaLevel" text="Arena Level 0" />
								<Label class="ArenaLevelPoints" id="ArenaLevelPoints" text="0 / 1000" />
								<Panel class="ArenaLevelPanelline">
									<Panel class="ArenaLevelPanelBG" />
									<Panel class="ArenaLevelPanelProgress" id="ArenaLevelPanelProgress" />
								</Panel>
								<Label class="LevelReward" html="true" text="#level_reward"/>
							</Panel>
						</Panel>
						<Panel class="QuestsProfile">
							<Panel class="QuestsProfileHeader">
								<Label class="QuestBlockLabel" text="#day_quests" />
							</Panel>
							<Panel class="QuestBlock" id="QuestBlock" />
						</Panel>
					</Panel>

					<Panel class="BlockChatWheel" onactivate="GameUI.CustomUIConfig().OpenChatWheel();">
						<Panel class="ChatWheelHeader">
							<Label class="ChatWheelHeaderLabel" text="#select_sounds" />
						</Panel>
						<Panel class="SoundBoard" />
					</Panel>
				</Panel>
				<Panel class="ProfileColumnEffects" >
					<Panel class="ChooseCustomizeBlock">
						<Label class="ChooseCustomizeBlockTitle" text="#customize_nickname" />
						<Panel class="ChooseList" id="NickNamePanel" >
						</Panel>
					</Panel>

					<Panel class="ChooseCustomizeBlock">
						<Label class="ChooseCustomizeBlockTitle" text="#customize_frame" />
						<Panel class="ChooseList" id="FramesPanel" >
						</Panel>
					</Panel>

					<Panel class="ChooseCustomizeBlock">
						<Label class="ChooseCustomizeBlockTitle" text="#customize_effect" />
						<Panel class="ChooseList" id="EffectsPanel" >
						</Panel>
					</Panel>
				</Panel>
			</Panel>

			<Panel class="DonateWindowBasic" id="DonateWindowBattleShop">
				<Panel class="DonateShopItems" id="DonateShopItems">

				</Panel>
			</Panel>
		</Panel>

		<Button class="BuyBattlePassWindow" id="BuyBattlePassWindow_coinshop" hittest="true">
			<Panel class="BuyBattlePassWindowHeader">
				<Label text="#bp_coin_shop" class="BuyBattlePassWindowHeaderLabel"/>	
				<Panel class="Close" onactivate="CloseBattlePassInfo('BuyBattlePassWindow_coinshop');"/>
			</Panel>
			<Panel class="CoinShop2Panels">
				<Panel class="CoinShopStolb">
					<Panel class="coinshop_line" onactivate="swap_currency(75, 1000);">
						<Panel style="flow-children:right;vertical-align:center;horizontal-align:center;">
							<Label class="DonateLineSum" text="#bp_coinshop_arena_coin_1_1" />
							<Panel class="DonateLineIconArena" />
							<Label class="DonateLineText" text="  —  " />
							<Label class="DonateLineText" text="#bp_coinshop_arena_coin_2_1" />
							<Panel class="DonateLineIconDonate" />
						</Panel>
					</Panel>
					<Panel class="coinshop_line" onactivate="swap_currency(150, 2000);">
						<Panel style="flow-children:right;vertical-align:center;horizontal-align:center;">
							<Label class="DonateLineSum" text="#bp_coinshop_arena_coin_1_2" />
							<Panel class="DonateLineIconArena" />
							<Label class="DonateLineText" text="  —  " />
							<Label class="DonateLineText" text="#bp_coinshop_arena_coin_2_2" />
							<Panel class="DonateLineIconDonate" />
						</Panel>
					</Panel>
					<Panel class="coinshop_line" onactivate="swap_currency(375, 5000);">
						<Panel style="flow-children:right;vertical-align:center;horizontal-align:center;">
							<Label class="DonateLineSum" text="#bp_coinshop_arena_coin_1_3" />
							<Panel class="DonateLineIconArena" />
							<Label class="DonateLineText" text="  —  " />
							<Label class="DonateLineText" text="#bp_coinshop_arena_coin_2_3" />
							<Panel class="DonateLineIconDonate" />
						</Panel>
					</Panel>
					<Panel class="coinshop_line" onactivate="swap_currency(750, 10000);">
						<Panel style="flow-children:right;vertical-align:center;horizontal-align:center;">
							<Label class="DonateLineSum" text="#bp_coinshop_arena_coin_1_4" />
							<Panel class="DonateLineIconArena" />
							<Label class="DonateLineText" text="  —  " />
							<Label class="DonateLineText" text="#bp_coinshop_arena_coin_2_4" />
							<Panel class="DonateLineIconDonate" />
						</Panel>
					</Panel>
					<Panel class="coinshop_line" onactivate="swap_currency(2250, 30000);">
						<Panel style="flow-children:right;vertical-align:center;horizontal-align:center;">
							<Label class="DonateLineSum" text="#bp_coinshop_arena_coin_1_5" />
							<Panel class="DonateLineIconArena" />
							<Label class="DonateLineText" text="  —  " />
							<Label class="DonateLineText" text="#bp_coinshop_arena_coin_2_5" />
							<Panel class="DonateLineIconDonate" />
						</Panel>
					</Panel>
				</Panel>
				<Panel class="CoinShopStolb">
					<Panel class="coinshop_line" onactivate="OpenDonateInfo();">
						<Panel style="flow-children:right;vertical-align:center;horizontal-align:center;">
							<Label class="DonateLineSum" text="#bp_coinshop_donate_coin_1_1" />
							<Panel class="DonateLineIconDonate" />
							<Label class="DonateLineText" text="  —  " />
							<Label class="DonateLineText" text="#bp_coinshop_donate_coin_2_1" />
							<Label class="DonateLineText" text="#bp_coinshop_currency_name" />
						</Panel>
					</Panel>
					<Panel class="coinshop_line" onactivate="OpenDonateInfo();">
						<Panel style="flow-children:right;vertical-align:center;horizontal-align:center;">
							<Label class="DonateLineSum" text="#bp_coinshop_donate_coin_1_2" />
							<Panel class="DonateLineIconDonate" />
							<Label class="DonateLineText" text="  —  " />
							<Label class="DonateLineText" text="#bp_coinshop_donate_coin_2_2" />
							<Label class="DonateLineText" text="#bp_coinshop_currency_name" />
						</Panel>
					</Panel>
					<Panel class="coinshop_line" onactivate="OpenDonateInfo();">
						<Panel style="flow-children:right;vertical-align:center;horizontal-align:center;">
							<Label class="DonateLineSum" text="#bp_coinshop_donate_coin_1_3" />
							<Panel class="DonateLineIconDonate" />
							<Label class="DonateLineText" text="  —  " />
							<Label class="DonateLineText" text="#bp_coinshop_donate_coin_2_3" />
							<Label class="DonateLineText" text="#bp_coinshop_currency_name" />
						</Panel>
					</Panel>
					<Panel class="coinshop_line" onactivate="OpenDonateInfo();">
						<Panel style="flow-children:right;vertical-align:center;horizontal-align:center;">
							<Label class="DonateLineSum" text="#bp_coinshop_donate_coin_1_4" />
							<Panel class="DonateLineIconDonate" />
							<Label class="DonateLineText" text="  —  " />
							<Label class="DonateLineText" text="#bp_coinshop_donate_coin_2_4" />
							<Label class="DonateLineText" text="#bp_coinshop_currency_name" />
						</Panel>
					</Panel>
					<Panel class="coinshop_line" onactivate="OpenDonateInfo();">
						<Panel style="flow-children:right;vertical-align:center;horizontal-align:center;">
							<Label class="DonateLineSum" text="#bp_coinshop_donate_coin_1_5" />
							<Panel class="DonateLineIconDonate" />
							<Label class="DonateLineText" text="  —  " />
							<Label class="DonateLineText" text="#bp_coinshop_donate_coin_2_5" />
							<Label class="DonateLineText" text="#bp_coinshop_currency_name" />
						</Panel>
					</Panel>
				</Panel>
			</Panel>
			<Label class="DonateCallbackInfo" html="true" text="#DonateCallbackInfo" />
		</Button>

		<Button id="donate_coin_panel">
			<Panel class="BuyBattlePassWindowHeader">
				<Label text="#bp_coin_shop" class="BuyBattlePassWindowHeaderLabel"/>	
				<Panel class="Close" onactivate="CloseDonateInfo();"/>
			</Panel>
			<Panel style="flow-children:right;horizontal-align:center;vertical-align:center;">
				<Panel style="flow-children:down;horizontal-align:center;vertical-align:center;" onactivate="ExternalBrowserGoToURL('https://qiwi.com/n/CUSTOMHEROARENA');">
					<Panel class="QiwiButton" />
					<Label class="QiwiButtonText" text="#donate_button_description_2" />
				</Panel>
				<Panel style="flow-children:down;horizontal-align:center;vertical-align:center;" onactivate="ExternalBrowserGoToURL('https://patreon.com/CustomHeroArena');">
					<Panel class="PatreonButton" />
					<Label class="QiwiButtonText" text="#donate_button_description_1" />
				</Panel>
			</Panel>
		</Button>

		<Panel id="donate_accept_window">
			<Panel style="align:center center;flow-children:down;">
				<Label id="donate_accept_label" text="#bp_accept" />
			</Panel>
		</Panel>

		<Panel id="donate_error_window">
			<Panel style="align:center center;flow-children:down;">
				<Label id="donate_error_label" text="#bp_error" />
			</Panel>
		</Panel>

		<Button id="donate_loading_window">
			<Panel style="align:center center;flow-children:down;">
				<Panel id="LoadingSpinner" />
				<Label id="donate_loading_label" text="#bp_loading" />
			</Panel>
		</Button>
	</Panel>
</root>


 


 




